
public class LinkedListReverse {

	public static void main(String[] args) {
		//Creation
		LinkedList<Integer> ll = new LinkedList<Integer>();
		Stack<Integer> st = new Stack<Integer>();
		
		//Addition
		for(int i=1; i<10; i++)
			ll.add(i);
		
		//Reversation
		int incrementer = 0;
		while(!ll.isEmpty())
			st.add(ll.get(incrementer++));
		
		
	}

}
